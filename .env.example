# Configuração de API Keys para Jarvis
# Copie este arquivo para .env e configure suas chaves

# Google Gemini API Key
# Obtenha em: https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here

# Groq API Key (opcional)
# Obtenha em: https://console.groq.com/
GROQ_API_KEY=your_groq_api_key_here

# Murf AI API Key (opcional)
# Para Text-to-Speech premium
MURF_API_KEY=your_murf_api_key_here

# Porcupine Access Key (opcional)
# Obtenha em: https://console.picovoice.ai/
PORCUPINE_ACCESS_KEY=your_porcupine_access_key_here

# Configurações de IP Camera
# Configure suas IP cameras aqui

# IP Camera Principal
IP_CAMERA_1_URL=http://192.168.1.100:8080/video
IP_CAMERA_1_USERNAME=
IP_CAMERA_1_PASSWORD=
IP_CAMERA_1_ENABLED=true

# IP Camera Secundária (RTSP)
IP_CAMERA_2_URL=rtsp://192.168.1.101:554/stream
IP_CAMERA_2_USERNAME=admin
IP_CAMERA_2_PASSWORD=password
IP_CAMERA_2_ENABLED=false

# Configurações de Câmera
CAMERA_DEFAULT_TYPE=auto  # auto, ip, local
CAMERA_DEFAULT_INDEX=0
CAMERA_WIDTH=640
CAMERA_HEIGHT=480